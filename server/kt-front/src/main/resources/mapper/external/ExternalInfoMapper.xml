<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.kt.mall.front.external.dao.ExternalInfoDAO">

    <cache type="cn.kt.mall.common.cache.MybatisRedisCache"/>

    <select id="getNationalCodes" resultType="cn.kt.mall.front.external.entity.NationalCodeEntity">
        SELECT
          *
        FROM
        tb_national_code
        ORDER BY id
    </select>

</mapper>